        "@angular/forms": "~12.1.0",
        "@angular/localize": "~12.1.0",
        "@angular/material": "^12.1.0",
        "@angular/platform-browser": "~12.1.0",
        "@angular/platform-browser-dynamic": "~12.1.0",
        "@angular/router": "~12.1.0",
        "@ckeditor/ckeditor5-angular": "^2.0.1",
        "@ckeditor/ckeditor5-build-decoupled-document": "^27.1.0",
        "@hardpool/ngx-spinner": "^2.2.1",
        "@ng-bootstrap/ng-bootstrap": "^8.0.4",
        "angular-responsive-carousel": "^2.0.2",
        "bootstrap": "^4.6.0",
        "ng-image-slider": "^2.8.0",
