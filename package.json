    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "clean": "rimraf ./dist",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --configuration production",
        "test": "ng test --watch=false",
        "test:watch": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build:ci": "npm run clean && npm run build:prod",
        "prepare": "husky install",
        "full-clean": "rimraf ./node_modules",
